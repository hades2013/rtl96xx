if [ -x "/etc/rc_boot_dsp" ]; then
        echo "run dsp set script..."
        /etc/rc_boot_dsp

        if [ -x "/bin/wait_dsp" ]; then
                wait_dsp
        else
                echo "no wait_dsp, so force to sleep 5 seconds..."
                sleep 5
        fi
fi


echo "start watch dog"
/bin/voip_gwdt /etc/app.gwdt &
