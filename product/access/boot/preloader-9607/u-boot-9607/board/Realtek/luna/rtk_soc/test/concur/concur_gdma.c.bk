/* MIC case 0 */
unsigned int GDMA_MIC_case0_setting(unsigned int burst)
{
        unsigned int data_size;
        unsigned int data_addr;

        _cache_flush_all();
        data_size = (sizeof(gdma_data)/8);
        data_addr = (unsigned int)gdma_data;

        /* Reset GDMA */
        REG32(GDMACNR) = 0 ;
        REG32(GDMACNR) = GDMA_ENABLE;
        REG32(GDMAIMR) = 0;
        REG32(GDMAISR) = 0xffffffff;
        REG32(GDMAICVL) = 0;
        REG32(GDMAICVR) = 0;
        REG32(GDMASBP0) = TO_PHY_ADDR(data_addr);
        REG32(GDMASBL0) = data_size;
        REG32(GDMASBP1) = TO_PHY_ADDR(data_addr+(1*data_size));
        REG32(GDMASBL1) = data_size;
        REG32(GDMASBP2) = TO_PHY_ADDR(data_addr+(2*data_size));
        REG32(GDMASBL2) = data_size;
        REG32(GDMASBP3) = TO_PHY_ADDR(data_addr+(3*data_size));
        REG32(GDMASBL3) = data_size;
        REG32(GDMASBP4) = TO_PHY_ADDR(data_addr+(4*data_size));
        REG32(GDMASBL4) = data_size;
        REG32(GDMASBP5) = TO_PHY_ADDR(data_addr+(5*data_size));
        REG32(GDMASBL5) = data_size;
        REG32(GDMASBP6) = TO_PHY_ADDR(data_addr+(6*data_size));
        REG32(GDMASBL6) = data_size;
        REG32(GDMASBP7) = TO_PHY_ADDR(data_addr+(7*data_size));
        REG32(GDMASBL7) = (GDMA_LDB|data_size);

	switch (burst){
		case 4:
        		return (GDMA_ENABLE|GDMA_POLL|GDMA_MIC);
			break;
		case 8:
        		return (GDMA_ENABLE|GDMA_POLL|GDMA_MIC|0x40);
			break;
		case 16:
	        	return (GDMA_ENABLE|GDMA_POLL|GDMA_MIC|0x80);
			break;
		default:
        		return (GDMA_ENABLE|GDMA_POLL|GDMA_MIC|0xC0);
			break;
	}
	return 0;
}

/* Check result data for MIC case 0 */
int GDMA_MIC_case0_checking(void)
{
        if(REG32(GDMAICVL)!= (0xa769eb37) || REG32(GDMAICVR) != (0xb1da5a0e)){
                printf("error ! ICVL = 0x%08x, ICVR = 0x%08x\n", REG32(GDMAICVL), REG32(GDMAICVR));
                return CHECKING_FAIL;
        }
        return CHECKING_PASS;
}

#define MEMSET_BUF_SIZE (0x800)

unsigned int gdma_memset_sbuf[MEMSET_BUF_SIZE];
unsigned int gdma_memset_dbuf[MEMSET_BUF_SIZE];
unsigned int gdma_memset_pat;

/* Memory case 0: Memory set */
unsigned int GDMA_Memory_case0_setting(unsigned int burst)
{
        unsigned int data_size;
        unsigned int data_addr_d;
	unsigned int i, mod_offset;
        volatile unsigned char *data_addr_s;

	gdma_memset_pat = 0x11223344;

	/* Clear source & distination buffer */
	for(i=0; i<sizeof(gdma_memset_sbuf)/sizeof(unsigned int);i++){
		gdma_memset_sbuf[i] = 0;
		gdma_memset_dbuf[i] = 0;
	}
	

        data_size = sizeof(gdma_memset_dbuf)/8;
        data_addr_d = (unsigned int)gdma_memset_dbuf;

        /* Reset GDMA */
        REG32(GDMACNR) = 0 ;
        REG32(GDMACNR) = GDMA_ENABLE;
        REG32(GDMAIMR) = 0;
        REG32(GDMAISR) = 0xffffffff;
        REG32(GDMAICVL) = gdma_memset_pat;
        REG32(GDMAICVR) = 0;
        REG32(GDMADBP0) = TO_PHY_ADDR(data_addr_d);
        REG32(GDMADBL0) = data_size-1;

	data_addr_s = (unsigned char *)gdma_memset_sbuf;
	for(i=0; i < (data_size-1); i++){
		*data_addr_s = (unsigned char)((gdma_memset_pat<<((i%4)*8))>>24);
		data_addr_s++;
	}
	mod_offset = (data_size-1)%4;

        REG32(GDMADBP1) = TO_PHY_ADDR(data_addr_d + (1*data_size));
        REG32(GDMADBL1) = data_size-2;

	data_addr_s = (unsigned char *)gdma_memset_sbuf;
	data_addr_s += data_size;
	for(i=0; i < (data_size-2); i++){
		*data_addr_s = (unsigned char)((gdma_memset_pat<<(((i+mod_offset)%4)*8))>>24);
		data_addr_s++;
	}
	mod_offset = (mod_offset + (data_size-2)%4)%4;

        REG32(GDMADBP2) = TO_PHY_ADDR(data_addr_d + (2*data_size));
        REG32(GDMADBL2) = data_size-3;

	data_addr_s = (unsigned char *)gdma_memset_sbuf;
	data_addr_s = data_addr_s + 2*data_size;
	for(i=0; i < (data_size-3); i++){
		*data_addr_s = (unsigned char)((gdma_memset_pat<<(((i+mod_offset)%4)*8))>>24);
		data_addr_s++;
	}
	mod_offset = (mod_offset + (data_size-3)%4)%4;

        REG32(GDMADBP3) = TO_PHY_ADDR(data_addr_d + (3*data_size));
        REG32(GDMADBL3) = data_size-4;

	data_addr_s = (unsigned char *)gdma_memset_sbuf;
	data_addr_s = data_addr_s + 3*data_size;
	for(i=0; i < (data_size-4); i++){
		*data_addr_s = (unsigned char)((gdma_memset_pat<<(((i+mod_offset)%4)*8))>>24);
		data_addr_s++;
	}
	mod_offset = (mod_offset + (data_size-4)%4)%4;


        REG32(GDMADBP4) = TO_PHY_ADDR(data_addr_d + (4*data_size));
        REG32(GDMADBL4) = data_size-5;

	data_addr_s = (unsigned char *)gdma_memset_sbuf;
	data_addr_s = data_addr_s + 4*data_size;
	for(i=0; i < (data_size-5); i++){
		*data_addr_s = (unsigned char)((gdma_memset_pat<<(((i+mod_offset)%4)*8))>>24);
		data_addr_s++;
	}
	mod_offset = (mod_offset + (data_size-5)%4)%4;

        REG32(GDMADBP5) = TO_PHY_ADDR(data_addr_d + (5*data_size));
        REG32(GDMADBL5) = data_size-6;

	data_addr_s = (unsigned char *)gdma_memset_sbuf;
	data_addr_s = data_addr_s + 5*data_size;
	for(i=0; i < (data_size-6); i++){
		*data_addr_s = (unsigned char)((gdma_memset_pat<<(((i+mod_offset)%4)*8))>>24);
		data_addr_s++;
	}
	mod_offset = (mod_offset + (data_size-6)%4)%4;

        REG32(GDMADBP6) = TO_PHY_ADDR(data_addr_d + (6*data_size));
        REG32(GDMADBL6) = data_size-7;

	data_addr_s = (unsigned char *)gdma_memset_sbuf;
	data_addr_s = data_addr_s + 6*data_size;
	for(i=0; i < (data_size-7); i++){
		*data_addr_s = (unsigned char)((gdma_memset_pat<<(((i+mod_offset)%4)*8))>>24);
		data_addr_s++;
	}
	mod_offset = (mod_offset + (data_size-7)%4)%4;

        REG32(GDMADBP7) = TO_PHY_ADDR(data_addr_d + (7*data_size));
        REG32(GDMADBL7) = (GDMA_LDB|(data_size-8));

	data_addr_s = (unsigned char *)gdma_memset_sbuf;
	data_addr_s = data_addr_s + 7*data_size;
	for(i=0; i < (data_size-8); i++){
		*data_addr_s = (unsigned char)((gdma_memset_pat<<(((i+mod_offset)%4)*8))>>24);
		data_addr_s++;
	}

        _cache_flush_all();

	switch (burst){
		case 4:
        		return (GDMA_ENABLE|GDMA_POLL|GDMA_MEMSET);
			break;
		case 8:
        		return (GDMA_ENABLE|GDMA_POLL|GDMA_MEMSET|0x40);
			break;
		case 16:
	        	return (GDMA_ENABLE|GDMA_POLL|GDMA_MEMSET|0x80);
			break;
		default:
        		return (GDMA_ENABLE|GDMA_POLL|GDMA_MEMSET|0xC0);
			break;
	}
	return 0;
}

/* Check result data for Memory case 0: memory set */
int GDMA_Memory_case0_checking(void)
{
	unsigned int i;

	for(i=0;i<sizeof(gdma_memset_sbuf)/sizeof(unsigned int);i++){
		if(gdma_memset_sbuf[i]!=gdma_memset_dbuf[i]){
			printf("(gdma_memset_sbuf[%d](addr 0x%08x):0x%08x) != (gdma_memset_dbuf[%d](addr 0x%08x):0x%08x) function %s, line %d\n",\
			i, &gdma_memset_sbuf[i], gdma_memset_sbuf[i], i, &gdma_memset_dbuf[i], gdma_memset_dbuf[i]);
			return CHECKING_FAIL;
		}
	}

        return CHECKING_PASS;
}


/* Pollng GDMA status */
int GDMA_nonb_polling(void)
{
        if ( ( REG32( GDMAISR ) & GDMA_COMPIP ) == 0 ){
                return IP_BUSY;
        }
        else
                return IP_IDLE;
}

/*
 * Dispatch GDMA Data setting
 * return value: 0 no such case. others, kick off value */
unsigned int GDMA_setting(int caseNo)
{
        switch (caseNo)
        {

                case 0:
                        return GDMA_MIC_case0_setting(4);
                        break;
                case 1:
                        return GDMA_MIC_case0_setting(8);
                        break;
                case 2:
                        return GDMA_MIC_case0_setting(16);
                        break;
                case 3:
                        return GDMA_MIC_case0_setting(32);
                        break;
                case 4:
                        return GDMA_Memory_case0_setting(4);
                        break;
                case 5:
                        return GDMA_Memory_case0_setting(8);
                        break;
                case 6:
                        return GDMA_Memory_case0_setting(16);
                        break;
                case 7:
                        return GDMA_Memory_case0_setting(32);
                        break;
                default:
                        return NO_SUCH_CASE;
                        break;
        }
        return NO_SUCH_CASE;
}
/*
 * Dispatch GDMA Data checking
 * return value: -1 checking data fail. 0 no such case. 1 checking data pased.*/
int GDMA_checking(int caseNo)
{
        switch (caseNo)
        {
                case 0:
                        return GDMA_MIC_case0_checking();
                case 1:
                        return GDMA_MIC_case0_checking();
                case 2:
                        return GDMA_MIC_case0_checking();
                case 3:
                        return GDMA_MIC_case0_checking();
                case 4:
                        return GDMA_Memory_case0_checking();
                case 5:
                        return GDMA_Memory_case0_checking();
                case 6:
                        return GDMA_Memory_case0_checking();
                case 7:
                        return GDMA_Memory_case0_checking();
                default:
                        return NO_SUCH_CASE;
        }
}
